{
  "manifest_version": 2,

  "name": "Nest",
  "description": "This extension tracks your Nest thermostat",
  "version": "0.9.4",
  "background": {
    "persistent": true,
    "scripts": [
      "assets/js/jquery-2.1.3.min.js",
      "assets/js/firebase.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "nest_ext_icon.png"
  },
  "permissions": [
    "tabs",
    "identity",
    "https://api.home.nest.com/",
    "https://developer-api.nest.com/"
  ]
}